{
  "action_manifest_version": 0,
  "alias_info": {},
  "app_key": "system.generated.classicube.exe",
  "bindings": {
    "/actions/main": {
      "haptics": [
        {
          "output": "/actions/main/out/haptic_hand_left",
          "path": "/user/hand/left/output/haptic"
        },
        {
          "output": "/actions/main/out/haptic_hand_right",
          "path": "/user/hand/right/output/haptic"
        }
      ],
      "poses": [
        {
          "output": "/actions/main/in/hand_left",
          "path": "/user/hand/left/pose/raw"
        },
        {
          "output": "/actions/main/in/hand_right",
          "path": "/user/hand/right/pose/raw"
        }
      ],
      "sources": [
        {
          "inputs": {
            "click": {
              "output": "/actions/main/in/place_block"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/trigger"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/main/in/delete_block"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/b"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/main/in/pick_block"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/joystick"
        },
        {
          "inputs": {
            "position": {
              "output": "/actions/main/in/walk_2_axis"
            }
          },
          "mode": "joystick",
          "path": "/user/hand/left/input/joystick"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/main/in/jump"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/a"
        },
        {
          "inputs": {
            "position": {
              "output": "/actions/main/in/turn_2_axis"
            }
          },
          "mode": "joystick",
          "path": "/user/hand/right/input/joystick"
        }
      ]
    }
  },
  "category": "steamvr_input",
  "controller_type": "oculus_touch",
  "description": "Bindings for ClassiCube for Oculus Touch (Quest 2)",
  "name": "ClassiCube bindings for Oculus Touch (Quest 2)",
  "options": {},
  "simulated_actions": []
}
